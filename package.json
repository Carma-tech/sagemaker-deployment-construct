    "aws-cdk": "2.1002.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "aws-cdk-lib": "^2.181.1",
    "aws-sdk": "^2.1692.0",
    "constructs": "^10.4.2",
    "uuid": "^11.1.0",
    "yaml": "^2.7.0"
  }
