{
  "name": "sagemaker-deployment-construct",
  "version": "0.1.0",
  "bin": {
    "sagemaker-deployment-construct": "bin/sagemaker-deployment-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
